// Storages are cyan.
// Storage modifiers are keywords, so bold yellow 8.
{
  syntax: {
    storage: {
      "*": {
        value: "{color.core.cyan.8}",
      },
      modifier: {
        "*": { value: "{color.core.yellow.8}", fontStyle: "bold" },
        // The namespace in an import.
        import: { value: "{color.core.black}", fontStyle: "none" },
      },
      type: {
        primitive: {
          value: "{color.core.cyan.8}",
          fontStyle: "bold"
        },
        class: {
          value: "{color.core.cyan.8}",
        },
        function: {
          value: "{color.core.blue.8}",
          fontStyle: "bold"
        },
      },
    },
  },
  semantic: {
    "namespace": {
      "*": { value: "{color.core.magenta.8}" },
    },
  },
}
